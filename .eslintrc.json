{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true,
		"jest": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"extends": ["plugin:prettier/recommended"],
	"plugins": ["prettier"],
	"prettier/prettier": [
		"error",
		{},
		{
			"usePrettierrc": false
		}
	],
	"arrow-body-style": "off",
	"prefer-arrow-callback": "off"
}
